{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently npm:watch-*",
    "watch-go": "air",
    "watch-vite": "vite",
    "watch-tailwind": "tailwindcss --watch -i static/input.css -o static/output.css",

    "build": "tsc && vite build && tailwindcss -i static/input.css -o dist/static/output.css",
    "preview": "vite preview",
    "format": "prettier src/**/*.tsx --write"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.2.7",
    "vite-plugin-solid": "^2.10.2"
  },
  "dependencies": {
    "@solidjs/router": "^0.13.1",
    "solid-js": "^1.8.16"
  }
}
